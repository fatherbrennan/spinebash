# Startup content relative from 'Resources/views/content/'
APP_HOME=index

# Include static view
USE_FRAME=true
USE_NAV=true

# Use defer script attribute in JavaScript files
JS_DEFER=false

# Script to run after spine build process (equal to 'spine <start|dev> && npm start')
SCRIPT_TAIL="npm start"

# Compress files
# Warning: This is in beta and only works for some use cases
COMPRESS_HTML=false
COMPRESS_CSS=true
COMPRESS_JS=false