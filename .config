# Startup content relative from 'Resources/views/content/'
APP_HOME=index

# Include static view
USE_FRAME=true
USE_NAV=true

# Use defer script attribute in JavaScript files
JS_DEFER=false

# Script to run after spine build process (equal to 'spine <start|dev> && npm start')
SCRIPT_TAIL="npm start"

#
# Asset Compression
#
# Compress files
COMPRESS_HTML=false
COMPRESS_CSS=false
COMPRESS_JS=false

# Compression scripts to run in place of the default framework compressors
# Info: Empty string or null value will default to framework compressors
# 'INPUT' placeholder will be replaced with the uncompressed file path
# 'OUTPUT' placeholder will be replaced with the compressed file path
#
# Examples:
# USE_COMPRESSOR_HTML="html-minifier-terser --collapse-whitespace --remove-comments INPUT > OUTPUT"
# USE_COMPRESSOR_JS="terser INPUT -o OUTPUT"
USE_COMPRESSOR_HTML=
USE_COMPRESSOR_CSS=
USE_COMPRESSOR_JS=